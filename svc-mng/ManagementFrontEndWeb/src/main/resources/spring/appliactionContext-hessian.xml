<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd"
	default-autowire="byName">

	<bean id="manageHessianServiceTarget" class="com.huateng.univer.hessian.impl.ManageHessianServiceImpl"
		scope="prototype">
		<property name="manageHessianInvoke">
			<ref bean="manageHessianInvoke" />
		</property>
		<property name="transactionManager">
			<ref bean="transactionManager" />
		</property>
		<property name="systemInfoBO">
			<ref bean="systemInfoBO" />
		</property>
	</bean>

	<bean id="manageHessianInvoke" class="com.huateng.univer.hessian.ManageHessianInvoke"
		scope="prototype">
	</bean>
	
</beans>

