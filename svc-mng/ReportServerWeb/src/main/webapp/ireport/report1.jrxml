	<subDataset name="inStorageDataSet">
		<parameter name="issuerId" class="java.lang.String"/>
		<queryString>
			<![CDATA[select to_date(t5.create_time,'yyyyMMddhh24miss') as datetime,
       t4.product_name as productName,
       sum(to_number(t1.face_value)/100) as faceValue,
       count(t1.card_no) as amount,
       t3.user_name as userName,
       t5.operater_type as operType
  from tb_entity_stock          t1,
       tb_issuer                t2,
       tb_ent_user              t3,
       tb_product               t4,
       tb_entity_stock_operater t5
 where t1.entity_id = t2.entity_id
   and t5.create_user = t3.user_id
   and t1.product_id=t4.product_id
   and t1.card_no=t5.card_no
   group by t5.create_time,t4.product_name,t3.user_name,t5.operater_type
   order by t5.create_time]]>
		</queryString>
		<field name="DATETIME" class="java.sql.Timestamp"/>
		<field name="PRODUCTNAME" class="java.lang.String"/>
		<field name="FACEVALUE" class="java.math.BigDecimal"/>
		<field name="AMOUNT" class="java.math.BigDecimal"/>
		<field name="USERNAME" class="java.lang.String"/>
		<field name="OPERTYPE" class="java.lang.String"/>
		<variable name="insumFaceValue" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{OPERTYPE}=="1"?$F{FACEVALUE}:new java.lang.Long("0")]]></variableExpression>
			<initialValueExpression><![CDATA[]]></initialValueExpression>
		</variable>
		<variable name="inSumAmount" class="java.lang.Long" calculation="Sum">
			<variableExpression><![CDATA[$F{OPERTYPE}=="1"?$F{AMOUNT}:new java.lang.Long("0")]]></variableExpression>
		</variable>
		<variable name="outSumFaceValue" class="java.math.BigDecimal" calculation="Sum">
			<variableExpression><![CDATA[$F{OPERTYPE}=="2"?$F{FACEVALUE}:new java.lang.Long("0")]]></variableExpression>
		</variable>
		<variable name="outSumAmount" class="java.lang.Long" calculation="Sum">
			<variableExpression><![CDATA[$F{OPERTYPE}=="2"?$F{AMOUNT}:new java.lang.Long("0")]]></variableExpression>
		</variable>
		<variable name="sumFaceValue" class="java.math.BigDecimal" calculation="Sum">
