	<queryString>
		<![CDATA[select to_date(substr(t.modify_time, 0, 8), 'yyyy-MM-dd') as datetime,
       t.order_id as orderId,
       decode(t.order_type, '30000001', '采购记名订单', '采购匿名订单') as orderType,
       t.total_price / 100 as talAmt,
       decode(t.card_issue_fee, null, t2.card_issue_fee, t.card_issue_fee) / 100 as cardFee,
       decode(t.annual_fee,null,0,t.annual_fee) / 100 as annualFee,
       decode(decode(t.package_fee, null, t2.package_fee, t.package_fee),
              null,
              0,
              decode(t.package_fee, null, t2.package_fee, t.package_fee)) / 100 as packageFee,
       decode(t.delivery_fee, null, 0, t.delivery_fee) / 100 as deliveryFee,
  decode(t.card_quantity,null,t2.CARD_AMOUNT,t.card_quantity) as amount,
       to_number(t.total_price/100) as al
  from tb_sell_order t, tb_sell_order_list t2
 where t.order_id = t2.order_id(+)
   and t2.data_state(+)=1
   and t.order_type in ('30000001', '30000002')
   and t.order_state = 27
   and t.process_entity_id = $P{key1}
   and t.first_entity_id=$P{issuerId}
   and t.order_id = $P{key3}]]>
	</queryString>
