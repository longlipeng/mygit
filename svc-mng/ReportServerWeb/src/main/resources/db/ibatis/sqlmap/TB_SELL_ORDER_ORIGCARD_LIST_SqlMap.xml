  <select id="selectOrigCardProdNames" parameterClass="java.lang.String" resultClass="java.util.HashMap" remapResults="true" >
  	select  distinct
		prod.product_name as "productName"
	from
		tb_sell_order_origcard_list orig,
		tb_product prod
	where
		orig.order_id=#orderId:VARCHAR#
	and
		orig.data_state='1'
	and
		orig.product_id=prod.product_id
	group by
