<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po.error">
	<class
		name="TblAlgoDtlCheck"
		table="tbl_algo_dtl_check"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			type="string"
			column="ID">
		<generator class="uuid.hex"/>
		</id><!-- UUID -->
		<property
			name="saState"
			column="SA_STATE"
			type="string"
			not-null="true"
			length="1"
		/><!-- 状态-->
		<property
			name="oprId"
			column="OPR_ID"
			not-null="true"
			length="14"
			type="string"
		/><!-- 操作者编号 -->
		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="string"
			not-null="true"
			length="14"
		/><!-- 修改时间 -->
		<property
			name="modifyOprId"
			column="MODIFY_OPR_ID"
			type="string"
			not-null="false"
			length="14"
		/><!--修改者编号 -->
		<property
			name="cutsInfo"
			column="CUTS_INFO"
			type="string"
			not-null="false"
			length="200"
		/><!--追扣原因 -->
		<property
			name="dateSettlmt"
			column="DATE_SETTLMT"
			type="string"
			not-null="true"
			length="8"
		/><!--清算日期 -->
		<property
			name="txnKey"
			column="TXN_KEY"
			type="string"
			not-null="true"
			length="48"
		/>
	</class>	
</hibernate-mapping>