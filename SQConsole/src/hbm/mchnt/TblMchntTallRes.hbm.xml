			not-null="false"
		/>
		<property
			name="lastUpdUser"
			column="last_upd_user"
			type="string"
			not-null="false"
		/>
		<property
			name="reserve1"
			column="reserve1"
			type="string"
			not-null="false"
		/>
		<property
			name="reserve2"
			column="reserve2"
			type="string"
			not-null="false"
		/>
		<property
			name="marketing_id"
			column="marketing_id"
			type="string"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>