<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po">
	<class
		name="TblDivMchnt"
		table="tbl_div_mchnt"
	>
		<meta attribute="sync-DAO">true</meta>
		<composite-id name="Id" class="TblDivMchntPK">
			<key-property
				name="MchtId"
				column="MCHT_ID"
				type="string"
			/>
			<key-property
				name="DivNo"
				column="DIV_NO"
				type="string"
			/>
		</composite-id>

		<property
			name="ProductCode"
			column="PRODUCT_CODE"
			type="string"
			not-null="true"
			length="20"
		/>
	</class>	
</hibernate-mapping>