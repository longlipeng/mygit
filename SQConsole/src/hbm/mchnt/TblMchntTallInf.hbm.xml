<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="as.huateng.po.management.mer">
	<class
		name="TblMchntTallInf"
		table="TBL_MCHNT_TALL_INF"
	>
		<composite-id name="Id" class="TblMchntTallInfPK">
			<key-property name="mchntId" column="mchnt_id" type="string" />
			<key-property name="feedbackTb" column="FEEDBACK_TB" type="string" />
		</composite-id>
		
		<property
			name="tbTradeVolume"
			column="tb_trade_volume"
			type="string"
			not-null="false"
		/>
		<property
			name="tbTradeCount"
			column="tb_trade_count"
			type="string"
			not-null="false"
		/>
		<property
			name="tbTradeRank"
			column="tb_trade_rank"
			type="string"
			not-null="false"
		/>
		<property
			name="tbTradeMccRank"
			column="tb_trade_mcc_rank"
			type="string"
			not-null="false"
		/>
		<property
			name="lastUpdTime"
			column="last_upd_time"
			type="string"
			not-null="false"
		/>
		<property
			name="lastUpdUser"
			column="last_upd_user"
			type="string"
			not-null="false"
		/>
		<property
			name="reserve1"
			column="reserve1"
			type="string"
			not-null="false"
		/>
		<property
			name="reserve2"
			column="reserve2"
			type="string"
			not-null="false"
		/>
		<property
			name="marketing_id"
			column="marketing_id"
			type="string"
			not-null="false"
		/>
	</class>	
</hibernate-mapping>