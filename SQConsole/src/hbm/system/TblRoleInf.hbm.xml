<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po">
	<class
		name="TblRoleInf"
		table="TBL_ROLE_INF"
	>
		<meta attribute="sync-DAO">true</meta>
		<id name="RoleId"
			column="ROLE_ID"
			type="integer"
		>
		<generator class="assigned" />
		</id>
		<property
			name="RoleName"
			column="ROLE_NAME"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="RoleType"
			column="ROLE_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="Misc1"
			column="MISC1"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Misc2"
			column="MISC2"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="RecUpdOpr"
			column="REC_UPD_OPR"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="RecCrtTs"
			column="REC_CRT_TS"
			type="string"
			not-null="true"
			length="14"
		/>
		<property
			name="RecUpdTs"
			column="REC_UPD_TS"
			type="string"
			not-null="false"
			length="14"
		/>
	</class>	
</hibernate-mapping>