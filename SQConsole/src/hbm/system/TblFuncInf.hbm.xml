<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po">
	<class
		name="TblFuncInf"
		table="TBL_FUNC_INF"
	>
		<meta attribute="sync-DAO">false</meta>
		<id name="FuncId"
			column="FUNC_ID"
			type="integer"
		>
		<generator class="assigned" />
		</id>
		<property
			name="FuncParentId"
			column="FUNC_PARENT_ID"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="FuncType"
			column="FUNC_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="FuncName"
			column="FUNC_NAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="PageName"
			column="PAGE_NAME"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="PageUrl"
			column="PAGE_URL"
			type="string"
			not-null="true"
			length="1024"
		/>
		<property
			name="iconPath"
			column="ICON_PATH"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="Misc1"
			column="MISC1"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Misc2"
			column="MISC2"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="Description"
			column="DESCRIPTION"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="RecUpdOpr"
			column="REC_UPD_OPR"
			type="string"
			not-null="false"
			length="40"
		/>
		<property
			name="RecCrtTs"
			column="REC_CRT_TS"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="RecUpdTs"
			column="REC_UPD_TS"
			type="string"
			not-null="false"
			length="14"
		/>
	</class>	
</hibernate-mapping>