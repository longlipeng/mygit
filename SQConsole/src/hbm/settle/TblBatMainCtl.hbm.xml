<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po.settle">
	<class
		name="TblBatMainCtl"
		table="TBL_BAT_MAIN_CTL"
	>
		<meta attribute="sync-DAO">true</meta>

		<id
			name="BatId"
			column="BAT_ID"
			type="string"
			length="5"
		/>
		<property
			name="BatProcName"
			column="BAT_PROC_NAME"
			type="string"
			not-null="true"
			length="16"
		/>
		<property
			name="BatCls"
			column="BAT_CLS"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="BatState"
			column="BAT_STATE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="RunProcQt"
			column="RUN_PROC_QT"
			type="string"
			not-null="true"
			length="2"
		/>
		<property
			name="CommitQt"
			column="COMMIT_QT"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="ChdCount"
			column="CHD_COUNT"
			type="integer"
			not-null="true"
			length="22"
		/>
		<property
			name="TaskAssignState"
			column="TASK_ASSIGN_STATE"
			type="string"
			not-null="true"
			length="1"
		/>
		<property
			name="TaskStartFlg"
			column="TASK_START_FLG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="BatDsp"
			column="BAT_DSP"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="BatNote"
			column="BAT_NOTE"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="RecOprId"
			column="REC_OPR_ID"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="RecUpdOpr"
			column="REC_UPD_OPR"
			type="string"
			not-null="false"
			length="11"
		/>
		<property
			name="RecCrtTs"
			column="REC_CRT_TS"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="RecUpdTs"
			column="REC_UPD_TS"
			type="string"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>