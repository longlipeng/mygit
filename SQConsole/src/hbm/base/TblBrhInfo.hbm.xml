			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="Resv2"
			column="RESV2"
			type="string"
			not-null="false"
			length="128"
		/>
		<property
			name="LastUpdOprId"
			column="LAST_UPD_OPR_ID"
			type="string"
			not-null="true"
			length="10"
