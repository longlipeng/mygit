			name="LastUpdTxnId"
			column="LAST_UPD_TXN_ID"
			type="string"
			not-null="true"
			length="10"
		/>
		<property
			name="LastUpdTs"
			column="LAST_UPD_TS"
			type="string"
			not-null="true"
			length="14"
		/>
	</class>	
</hibernate-mapping>