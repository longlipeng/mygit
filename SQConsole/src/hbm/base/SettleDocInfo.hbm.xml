<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po">
	<class
		name="SettleDocInfo"
		table="SETTLE_DOC_INFO"
	>
		<id
			name="Id"
			type="string"
			column="BRH_ID"
		>
		</id>

		<property
			name="ExchangeNo"
			column="EXCHANGE_NO"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="PayerName"
			column="PAYER_NAME"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="PayerAccountNo"
			column="PAYER_ACCOUNT_NO"
			type="string"
			not-null="false"
			length="21"
		/>
		<property
			name="PayBankNo"
			column="PAY_BANK_NO"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="InBankSettleNo"
			column="IN_BANK_SETTLE_NO"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="SettleBusNo"
			column="SETTLE_BUS_NO"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="InnerAcct"
			column="INNER_ACCT"
			type="string"
			not-null="false"
			length="28"
		/>
		<property
			name="InnerAcct1"
			column="INNER_ACCT1"
			type="string"
			not-null="false"
			length="28"
		/>
		<property
			name="InnerAcct2"
			column="INNER_ACCT2"
			type="string"
			not-null="false"
			length="28"
		/>
		<property
			name="Reserved"
			column="RESERVED"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="RecUpdUser"
			column="REC_UPD_USER"
			type="string"
			not-null="false"
			length="12"
		/>
		<property
			name="RecUpdTs"
			column="REC_UPD_TS"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="RecCrtTs"
			column="REC_CRT_TS"
			type="string"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>