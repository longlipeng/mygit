		<property
			name="SaLimitAmt"
			column="SA_LIMIT_AMT"
			type="string"
			length="12"
		/><!-- 受控金额 -->
		<property
			name="SaAction"
			column="SA_ACTION"
			type="string"
			length="1"
		/><!-- 受控动作 -->
		<property
			name="SaInitZoneNo"
			column="SA_INIT_ZONE_NO"
			type="string"
			length="6"
		/><!-- 创建分行 -->
		<property
			name="SaInitOprId"
			column="SA_INIT_OPR_ID"
			type="string"
			length="40"
		/><!-- 创建人ID -->	
		<property
			name="SaModiZoneNo"
			column="SA_MODI_ZONE_NO"
			type="string"
			length="6"
		/><!-- 修改分行 -->
		<property
			name="SaModiOprId"
			column="SA_MODI_OPR_ID"
			type="string"
			length="40"
