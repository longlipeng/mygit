		<property
			name="SaMerEnName"
			column="SA_MER_EN_NAME"
			type="string"
			length="60"
		/><!-- 英文名称 -->
		<property
			name="SaZoneNo"
			column="SA_ZONE_NO"
			type="string"
			length="6"
		/><!-- 分公司号 -->
		<property
			name="SaAdmiBranNo"
			column="SA_ADMI_BRAN_NO"
			type="string"
			length="6"
