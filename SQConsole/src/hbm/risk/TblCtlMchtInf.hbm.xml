			name="addType"
			column="ADD_TYPE"
			type="string"
		/><!-- 添加方式 --><property
			name="managerTel"
			column="MANAGER_TEL"
			type="string"
		/><!-- 法人联系方式 -->
		<property
			name="appRemark"
			column="APP_REMARK"
			type="string"
		/><!-- 申请备注 -->
		<property
			name="saArea"
			column="SA_AREA"
			length="60"
			type="string"
		/><!-- 地区 -->
	</class>	
</hibernate-mapping>