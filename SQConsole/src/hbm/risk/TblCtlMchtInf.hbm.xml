		<property
			name="SaModiTime"
			column="SA_MODI_TIME"
			type="string"
			length="14"
		/><!-- 修改时间 -->
		<property name="SaState"
			      column="SA_STATE"
			      type="string"
			      length="1">
		</property><!-- 状态 -->
		<property name="SaLimitAmtOld"
			      column="SA_LIMIT_AMT_OLD"
			      type="string"
			      length="12">
		</property><!-- 受控金额 -->
		<property name="SaActionOld"
			      column="SA_ACTION_OLD"
			      type="string"
			      length="1">
		</property><!-- 受控动作 -->
		<property
			name="BankLicenceNo"
			column="BANK_LICENCE_NO"
			type="string"
			length="20"
		/><!-- 机构代码证号码 -->
		<property
			name="LicenceNo"
			column="LICENCE_NO"
			type="string"
			length="20"
		/><!-- 营业执照号码 -->
		<property
			name="IdentityNo"
			column="IDENTITY_NO"
			type="string"
			length="20"
