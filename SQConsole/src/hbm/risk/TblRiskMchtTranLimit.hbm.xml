<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po.risk">
	<class
		name="TblRiskMchtTranLimit"
		table="tbl_risk_mcht_tran_limit"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		<generator class="assigned"/>
		</id><!-- UUID -->
		<property
			name="saState"
			column="SA_STATE"
			type="string"
			not-null="true"
			length="1"
		/><!-- 状态-->
		<property
			name="mchtNo"
			column="MCHT_NO"
			not-null="true"
			length="15"
			type="string"
		/><!-- 操作员 ID-->
		<property
			name="updateTime"
			column="UPDATETIME"
			type="string"
		/><!-- 修改时间-->
		<property
			name="txnNum"
			column="TXN_NUM"
			type="string"
			not-null="true"
			length="4"
		/><!--交易代码 -->
		<property
			name="bussType"
			column="BUSS_TYPE"
			type="string"
			not-null="true"
			length="6"
		/><!--业务类型 -->
		<property
			name="channel"
			column="CHANNEL"
			type="string"
			not-null="true"
			length="2"
		/><!-- 交易渠道 -->
		<property
			name="cardType"
			column="CARD_TYPE"
			type="string"
			not-null="true"
			length="2"
		/><!-- 卡类型 -->
		<property
			name="limit"
			column="LIMIT"
			type="string"
			not-null="true"
			length="2"
		/><!-- 权限 -->
		<property
			name="txnNumOld"
			column="TXN_NUM_OLD"
			type="string"
			not-null="false"
			length="4"
		/><!--修改前的交易代码 -->
		<property
			name="bussTypeOld"
			column="BUSS_TYPE_OLD"
			type="string"
			not-null="false"
			length="6"
		/><!--修改前的业务类型 -->
		<property
			name="channelOld"
			column="CHANNEL_OLD"
			type="string"
			not-null="false"
			length="2"
		/><!-- 修改前的交易渠道 -->
		<property
			name="cardTypeOld"
			column="CARD_TYPE_OLD"
			type="string"
			not-null="false"
			length="2"
		/><!-- 修改前的卡类型 -->
		<property
			name="limitOld"
			column="LIMIT_OLD"
			type="string"
			not-null="false"
			length="2"
		/><!-- 修改前的权限 -->
			<property
			name="creOprId"
			column="CRE_ID"
			type="string"
		/>
			<property
			name="creTime"
			column="CRETIME"
			type="string"
		/>
			<property
			name="OprId"
			column="OPR_ID"
			type="string"
		/>
	</class>	
</hibernate-mapping>