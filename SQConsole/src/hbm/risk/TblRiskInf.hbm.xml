<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.huateng.po">
	<class
		name="TblRiskInf"
		table="tbl_risk_inf"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="sa_model_kind"
			length="2"
		>
			<generator class="assigned"/>
		</id><!-- 模型 -->
		<property
			name="SaDays"
			column="sa_days"
			type="string"
			length="3"
		/><!-- 有效天数 -->
		<property
			name="SaBeUse"
			column="sa_be_use"
			type="string"
			length="1"
		/><!-- 是否启用 -->
		<property
			name="SaAction"
			column="sa_action"
			type="string"
			length="1"
		/><!-- 风险处理动作 -->
		<property
			name="SaLimitNum"
			column="sa_limit_num"
			type="string"
			length="8"
		/><!-- 受控笔数 -->
		<property
			name="SaLimitAmount"
			column="sa_limit_amount"
			type="string"
			length="12"
		/><!-- 受控金额 -->
		<property
			name="SaWarnlvt"
			column="sa_warnlvt"
			type="int"
		/><!-- 受控级别 -->
		<property
			name="ModiZoneNo"
			column="modi_zone_no"
			type="string"
			length="10"
		/><!-- 修改分行 -->
		<property
			name="ModiOprId"
			column="modi_opr_id"
			type="string"
			length="40"
		/><!-- 修改人ID -->
		<property
			name="ModiTime"
			column="modi_time"
			type="string"
			length="14"
		/><!-- 修改时间 -->
		<property
			name="SaBranchCode"
			column="sa_branch_code"
			type="string"
			length="10"
		/><!-- 分行号 -->
		<property
			name="SaBeUseModify"
			column="sa_be_use_modify"
			type="string"
			not-null="false"
			length="1"
		/><!-- 是否启用(修改成为的值) -->
		<property
			name="SaActionModify"
			column="sa_action_modify"
			type="string"
			not-null="false"
			length="1"
		/><!-- 风险处理动作(修改成为的值) -->
		<property
			name="SaLimitNumModify"
			column="sa_limit_num_modify"
			type="string"
			not-null="false"
			length="8"
		/><!-- 受控笔数(修改成为的值) -->
		<property
			name="SaLimitAmountModify"
			column="sa_limit_amount_modify"
			type="string"
			not-null="false"
			length="12"
		/><!-- 受控金额(修改成为的值) -->
		<property
			name="SaDaysModify"
			column="sa_days_modify"
			type="string"
			not-null="false"
			length="3"
		/><!-- 有效天数(修改成为的值) -->
		<property
			name="SaState"
			column="sa_state"
			type="string"
			length="1"
		/><!-- 审核状态 -->
		
		<property
			name="saModelName"
			column="SA_MODEL_NAME"
			type="string"
			length="64"
		/><!-- 受控笔数 -->
	</class>	
</hibernate-mapping>