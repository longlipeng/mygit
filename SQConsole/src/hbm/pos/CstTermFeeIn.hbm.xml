			name="monamt"
			column="MON_AMT"
			type="java.lang.Double"
		/>
		<property
			name="reccrtts"
			column="REC_CRT_TS"
			type="string"
		/>
        <property
			name="recupdts"
			column="REC_UPD_TS"
			type="string"
		/>
		<property
			name="upid"
			column="UP_ID"
			type="string"
		/>
        <property
			name="creid"
			column="CRE_ID"
			type="string"
		/>
		<property
			name="sastatue"
			column="SA_STATE"
			type="string"
		/>
		<property
			name="saaction"
			column="SA_ACTION"
			type="string"
		/>
	</class>	
</hibernate-mapping>