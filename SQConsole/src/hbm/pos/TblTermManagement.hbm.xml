		<id
			name="Id"
			type="string"
			column="TERM_NO"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="MechNo"
			column="MECH_NO"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="TermType"
			column="TERM_TYPE"
			type="string"
			not-null="true"
			length="1"
		/>
