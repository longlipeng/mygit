<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <constant name="struts.objectFactory" value="spring" />
	<package name="reserve" extends="json-default">
		
		<!-- 系统拦截器 -->
		<interceptors>
			<interceptor name="logInter" class="com.huateng.interceptor.LogInter"></interceptor>
			<interceptor-stack name="SystemInterceptor">
				<interceptor-ref name="defaultStack"></interceptor-ref>
				<interceptor-ref name="logInter"></interceptor-ref>
			</interceptor-stack>
		</interceptors>
		
		<!-- 公用返回类型 -->
		<global-results>
			<result type="plainText" >
                <param name="charSet">UTF-8</param>
                <param name="location">/page/system/emptyLocation.jsp</param>
            </result>
		</global-results>
		
		<!-- 批处理控制 
        <action name="QryBatMainAction" class="qryBatMainAction"/>-->
        
		<!-- 备出款 -->
		<action name="T9*_*" 
			class="com.huateng.struts.reserve.action.T9{1}" method="{2}">
			<interceptor-ref name="SystemInterceptor"></interceptor-ref>
			<result name="success" type="json">
				<param name="includeProperties">success,msg</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,msg</param>
			</result>
		</action>
		
		<!-- 回调接口 -->
		<action name="backRcvResponse" class="com.huateng.struts.reserve.action.BackRcvResponse" />
			
		<!--账户白名单 -->
		<!-- <action name="T91101Action" class="com.huateng.struts.reserve.action.T91101Action" /> -->

	</package>
</struts>