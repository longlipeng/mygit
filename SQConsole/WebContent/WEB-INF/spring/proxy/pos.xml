        <property name="t3030BO">
            <ref bean="t3030BO"/>
        </property>
        <property name="methods">
            <map>
                <!-- 通过 -->
                <entry key="00" value="reciTerminal"/>
                <!-- 批次通过-->
                <entry key="01" value="reciTerminals"/>
                <!-- 拒绝 -->
                <entry key="10" value="refuseTerminal"/>
                <!-- 批次拒绝 -->
                <entry key="11" value="refuseTerminals"/>
           </map>
        </property>
    </bean>
    <bean id="t30304Action" class="com.huateng.struts.pos.action.T30304Action" singleton="false">
