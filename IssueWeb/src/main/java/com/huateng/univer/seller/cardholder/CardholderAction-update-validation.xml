<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
    <field name="cardholderDTO.entityId">
        <field-validator type="requiredstring" short-circuit="true">
            <message>请选择客户号！</message>
        </field-validator>
    </field>
    <field name="cardholderDTO.firstName">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请输入持卡人姓名！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">20</param>
			<message>持卡人姓名不能超过20个字符！</message>
		</field-validator>		
	</field>	
	<field name="cardholderDTO.idType">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择证件类型！</message>
		</field-validator>
	</field>
    <field name="cardholderDTO.idNo">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请输入证件号码！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">18</param>
			<message>证件号码不能超过18个字符！</message>
		</field-validator>		
	</field>
	<field name="cardholderDTO.validity">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请输入证件失效日期！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.country">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择国籍！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderEducation">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择学历！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderNation">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择民族！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderMarriage">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择婚姻状况！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.profession">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择职业！</message>
		</field-validator>
	</field>
	
	<field name="cardholderDTO.cardholderMobile">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请输入手机号码！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">11</param>
			<message>手机号码不能超过11个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.mailingAddress">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请输入邮寄地址！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">75</param>
			<message>邮寄地址不能超过75个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderGender">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请选择性别！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderBirthday">
		<field-validator type="requiredstring" short-circuit="true">
			<message>请输入出生日期！</message>
		</field-validator>		
	</field>
	<field name="cardholderDTO.city">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">15</param>
			<message>城市不能超过15个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderEmail">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">32</param>
			<message>邮箱地址不能超过32个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.v_Id">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">15</param>
			<message>车架号不能超过15个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.plateNumber">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">7</param>
			<message>车牌号不能超过7个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.driverLicence">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">18</param>
			<message>驾驶证号不能超过18个字符！</message>
		</field-validator>
	</field>
	<field name="cardholderDTO.cardholderComment">
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">1024</param>
			<message>备注不能超过1024个字符！</message>
		</field-validator>
	</field>		
</validators>