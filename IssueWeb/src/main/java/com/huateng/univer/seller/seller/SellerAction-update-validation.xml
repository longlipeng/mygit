    <field name="sellerDTO.externalId">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">64</param>
            <message>外部系统代码不能超过64个字符！</message>
        </field-validator>
    </field>
    <field name="sellerDTO.legCusId">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">64</param>
            <message>老系统客户号不能超过64个字符！</message>
        </field-validator>
    </field>
    

    
