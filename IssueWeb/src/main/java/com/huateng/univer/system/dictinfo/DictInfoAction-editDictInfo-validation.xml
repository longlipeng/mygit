<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>

	<field name="dictInfoDTO.dictTypeCode">
		<field-validator type="requiredstring">
			<message>必须选择</message>
		</field-validator>
				<field-validator type="regex">
			<param name="expression">^([1-9][0-9]{0,4}|[0])$</param>
			<message>必须是正整数(5位内)</message>
		</field-validator>
	</field>
	
	<field name="dictInfoDTO.dictName">
		<field-validator type="requiredstring">
			<message>请填写名称(32位内)</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">32</param>
			<message>请填写名称(32位内)</message>
		</field-validator>
	</field>
	
	
	<field name="dictInfoDTO.dictShortName">
		<field-validator type="requiredstring">
			<message>请填简称(32位内)</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">32</param>
			<message>请填简称(32位内)</message>
		</field-validator>
	</field>
	
		
	<field name="dictInfoDTO.dictEnglishName">
		<field-validator type="regex">
			<param name="expression">^[a-zA-Z_ ]+$</param>
			<message>如果填写必须是英文(32位内)</message>
		</field-validator>
	</field>
	
	<field name="dictInfoDTO.fatherDictIdStr">
		<field-validator type="regex">
			<param name="expression">^([1-9][0-9]{0,4}|[0])$</param>
			<message>如果填写必须是正整数字</message>
		</field-validator>
	</field>
	
	
</validators>