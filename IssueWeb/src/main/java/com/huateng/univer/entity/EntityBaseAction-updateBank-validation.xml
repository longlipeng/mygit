<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
	<field name="bankDTO.bankName">
		<field-validator type="requiredstring" short-circuit="true">
			<message>银行名称必须输入！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">128</param>
			<message>银行名称不能超过128个字！</message>
		</field-validator>
	</field>

	<field name="bankDTO.bankAccount">
		<field-validator type="requiredstring" short-circuit="true">
			<message>银行账户必须输入！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">32</param>
			<message>银行账户不能超过32个字！</message>
		</field-validator>
	</field>

	<field name="bankDTO.bankAccountName">
		<field-validator type="requiredstring" short-circuit="true">
			<message>银行账户名称必须输入！</message>
		</field-validator>
		<field-validator type="stringlength" short-circuit="true">
			<param name="maxLength">32</param>
			<message>银行账户名称长度不能超过32个字！</message>
		</field-validator>
	</field>

</validators>