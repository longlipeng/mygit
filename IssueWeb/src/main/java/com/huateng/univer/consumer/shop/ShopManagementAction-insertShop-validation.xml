    <field name="shopDTO.shopTelephone">
        <field-validator type="requiredstring" short-circuit="true">
            <message>电话必须输入！</message>
        </field-validator>
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">32</param>
            <message>电话不能超过32个字符！</message>
        </field-validator>
        <field-validator type="regex" short-circuit="true">
            
            <message>电话格式不正确,正确格式是"区号-电话号码"或者只输电话号码！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopFax">
        <field-validator type="requiredstring" short-circuit="true">
            <message>传真必须输入！</message>
        </field-validator>
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">32</param>
            <message>传真不能超过32个字符！</message>
        </field-validator>
        <field-validator type="regex" short-circuit="true">
            
            <message>传真格式不正确,传真格式是"区号-传真"或者只输传真！</message>
        </field-validator>
    </field>
    <field name="shopDTO.website">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">128</param>
            <message>网站不能超过128个字符！</message>
        </field-validator>
        <field-validator type="url" short-circuit="true">
             <param name="expression"><![CDATA[^(http[s]?:\/\/)?[A-Za-z0-9]+\.[A-Za-z0-9]+[\/=\?%\-&_~`@\[\]\':+!]*([^\"\"])*$]]></param>
            <message>网站不符合URL格式,网址格式必须形如http://www.xxxx.com</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopCity">
        <field-validator type="required" short-circuit="true">
            <message>城市必须选择！</message>
        </field-validator>
    </field>
    <field name="shopDTO.businessArea">
        <field-validator type="required" short-circuit="true">
            <message>区域必须选择！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopCategory">
        <field-validator type="required" short-circuit="true">
            <message>门店类型必须选择！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopType">
        <field-validator type="required" short-circuit="true">
            <message>门店子类必须选择！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopRank">
        <field-validator type="required" short-circuit="true">
            <message>门店等级必须选择！</message>
        </field-validator>
    </field>
    <field name="shopDTO.paymentType">
        <field-validator type="required" short-circuit="true">
            <message>支付类型必须选择！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopLongitude">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">16</param>
            <message>门店经度不能超过16个字符！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopLatitude">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">16</param>
            <message>门店纬度不能超过16个字符！</message>
        </field-validator>
    </field>
    <field name="shopDTO.memo">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">1024</param>
            <message>备注不能超过1024个字符！</message>
        </field-validator>
    </field>
    <field name="shopDTO.shopNotes">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">1024</param>
            <message>门店介绍不能超过1024个字符！</message>
        </field-validator>
    </field>
    <field name="shopDTO.englishNotes">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">1024</param>
            <message>英文介绍不能超过1024个字符！</message>
        </field-validator>
    </field>
    <field name="shopDTO.joinDate">
        <field-validator type="required" short-circuit="true">
            <message>加盟日期必须选择！</message>
        </field-validator>
    </field>
</validators>