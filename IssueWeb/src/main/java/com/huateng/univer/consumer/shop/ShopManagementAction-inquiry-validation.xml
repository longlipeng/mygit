<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>
    <field name="shopQueryDTO.shopId">
        <field-validator type="conversion" short-circuit="true">
            <message>门店号必须是数字！</message>
        </field-validator>
        <field-validator type="stringlength" short-circuit="true">
            <param name="max">9999999999</param>
            <message>门店号不能超过10位！</message>
        </field-validator>
    </field>

    <field name="shopQueryDTO.shopName">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">128</param>
            <message>门店名称不能超过128个字符！</message>
        </field-validator>
    </field>
</validators>