<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>	
		<field name="outerPosInfoDTO.termId">
 			<field-validator type="requiredstring">
				<message>终端号不能为空</message>
			</field-validator>
			<field-validator type="stringlength">
			<param name="minLength">8</param>
			<message>终端号必须是8位</message>
		</field-validator>
    	</field>
 	<field name="outerPosInfoDTO.mchntId">
 		<field-validator type="requiredstring">
			<message>商户号不能为空</message>
		</field-validator>
    </field> 	
 	<field name="outerPosInfoDTO.mchntName">
 		<field-validator type="requiredstring">
			<message>商户名称不能为空</message>
		</field-validator>
    </field>
 		 	<field name="outerPosInfoDTO.shopId">
 		<field-validator type="requiredstring">
			<message>门店号不能为空</message>
		</field-validator>
    </field>
    	<field name="outerPosInfoDTO.shopName">
 		<field-validator type="requiredstring">
			<message>门店名称不能为空</message>
		</field-validator>
    </field> 
  
    	<field name="outerPosInfoDTO.termStat">
 		<field-validator type="requiredstring">
			<message>终端状态不能为空</message>
		</field-validator>
    </field> 	
 	<field name="outerPosInfoDTO.termOwner">
 		<field-validator type="requiredstring">
			<message>终端持有者不能为空</message>
		</field-validator>
    </field>
</validators>