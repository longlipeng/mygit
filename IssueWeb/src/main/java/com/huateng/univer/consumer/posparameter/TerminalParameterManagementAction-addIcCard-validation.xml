<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">
<validators>	
 	<field name="posParameterDTO.prmDesc">
        <field-validator type="stringlength" short-circuit="true">
            <param name="maxLength">64</param>
            <message>不超过64位</message>
        </field-validator>
    </field>
	<field name="posParameterDTO.prmName">
		<field-validator type="requiredstring">
			<message>请填写参数名称</message>
		</field-validator>
    </field>
	 <field name="posParameterDTO.prmVersion">
       <field-validator type="requiredstring">
			<message>请填写5位内的正整数</message>
		</field-validator>
    </field> 
 	
 	 <field name="posParameterDTO.prmVal">
       <field-validator type="requiredstring">
			<message>参数值必须填写</message>
		</field-validator>		
    </field> 
 	
 	
</validators>