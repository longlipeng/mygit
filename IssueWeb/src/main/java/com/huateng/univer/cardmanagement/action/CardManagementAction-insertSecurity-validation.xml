<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.3//EN"
        "http://struts.apache.org/dtds/xwork-validator-1.0.3.dtd">

<validators>
	
	<field name="cardManagementDTO.serviceFee">
		<field-validator type="required">
			<message>请正确填写服务费不能多于10位数字</message>
		</field-validator>
		<field-validator type="long">
			<param name="max">9999999999</param>
			<param name="min">0</param>
			<message>请正确填写服务费不能多于10位数字</message>
		</field-validator>
	</field>
	<field name="cardManagementDTO.posSingleAmount">
		<field-validator type="required">
			<message>请正确填写POS支付单笔限额,不能多于10位的数字</message>
		</field-validator>
		<field-validator type="long">
			<param name="max">9999999999</param>
			<param name="min">0</param>
			<message>请正确填写POS支付单笔限额,不能多于10位的数字</message>
		</field-validator>
	</field>
	<field name="cardManagementDTO.posDailyAmount">
		<field-validator type="required">
			<message>请正确填写POS支付日限额,不能多于10位的数字</message>
		</field-validator>
		<field-validator type="long">
			<param name="max">9999999999</param>
			<param name="min">0</param>
			<message>请正确填写POS支付日限额,不能多于10位的数字</message>
		</field-validator>
	</field>
	<field name="cardManagementDTO.webSingleAmount">
		<field-validator type="required">
			<message>请正确填写网上支付单笔限额,不能多于10位的数字</message>
		</field-validator>
		<field-validator type="long">
			<param name="max">9999999999</param>
			<param name="min">0</param>
			<message>请正确填写网上支付单笔限额,不能多于10位的数字</message>
		</field-validator>
	</field>
	<field name="cardManagementDTO.webDailyAmount">
		<field-validator type="required">
			<message>请正确填写网上支付日限额,不能多于10位的数字</message>
		</field-validator>
		<field-validator type="long">
			<param name="max">9999999999</param>
			<param name="min">0</param>
			<message>请正确填写网上支付日限额,不能多于10位的数字</message>
		</field-validator>
	</field>
	<field name="cardManagementDTO.withoutPinAmount">
		<field-validator type="required">
			<message>请正确无需密码限额,不能多于10位的数字</message>
		</field-validator>
		<field-validator type="long">
			<param name="max">9999999999</param>
			<param name="min">0</param>
			<message>请正确无需密码限额,不能多于10位的数字</message>
		</field-validator>
	</field>
</validators>