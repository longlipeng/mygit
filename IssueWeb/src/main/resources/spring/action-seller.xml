<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
	default-autowire="byName">

	<!-- 营销机构action配置文件 -->
	<!-- 营销机构信息-->
	<bean id="sellerAction" class="com.huateng.univer.seller.seller.SellerAction" scope="prototype"> </bean>
	<!-- 营销机构合同 -->
	<bean id="sellerContractAction" class="com.huateng.univer.seller.sellercontract.SellerContractAction" scope="prototype"></bean>
	<!-- 客户信息 -->
	<bean id="customerAction" class="com.huateng.univer.seller.customer.CustomerManagementAction" scope="prototype"> </bean>
	<!-- 客户合同 -->
	<bean id="customerContractAction" class="com.huateng.univer.seller.customercontract.CustomerContractAction" scope="prototype"> </bean>
	<!-- 持卡人信息 -->
	<bean id="cardholderAction" class="com.huateng.univer.seller.cardholder.CardholderAction" scope="prototype"/>

	<!-- 客户审核 -->
	 <bean id="customerVerifiterAction" class="com.huateng.univer.seller.customerverifier.CustomerVerifierAction" scope="prototype"/>

	<!-- 会员信息 -->
	<bean id="memberManagementAction" class="com.huateng.univer.seller.member.MemberManagementAction" scope="prototype"/>
	 
    <!-- 会员信息 -->
	<bean id="orderInvoiceAction" class="com.huateng.univer.seller.invoice.OrderInvoiceAction" scope="prototype"/>
	
	<!-- 发票收交查询-->
	<bean id="invoiceReceiveAction" class="com.huateng.univer.seller.invoice.InvoiceReceiveAction" scope="prototype"/>
	<!-- 批量导入客户信息和持卡人信息 -->
	<bean id="batchImportCardHolderAndCustomerAction" class="com.allinfinance.batchImport.BatchImportCustomerAndCardHolder" scope="prototype"/>
</beans>