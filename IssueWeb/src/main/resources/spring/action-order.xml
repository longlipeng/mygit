<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
	default-autowire="byName">

	<!-- 订单查询 -->
	<bean id="orderQueryAction"
		class="com.huateng.univer.seller.sellorder.OrderQueryAction"
		scope="prototype">
	</bean>
	

	<!-- 订单录入 -->
	<bean id="orderInputAction"
		class="com.huateng.univer.seller.sellorder.OrderInputAction"
		scope="prototype">
	</bean>
	
	<!-- 订单确定 -->
	<bean id="orderConfirmAction"
		class="com.huateng.univer.seller.sellorder.OrderConfirmAction"
		scope="prototype">
	</bean>
	<!-- 订单准备 -->
	
	<bean id="orderReadyAction"
		class="com.huateng.univer.seller.sellorder.OrderReadyAction"
		scope="prototype">
	</bean>
	
	<bean id="orderAcceptAction"
		class="com.huateng.univer.seller.sellorder.OrderAcceptAction"
		scope="prototype">
	</bean>
	
	
	<!-- 订单配送 -->
	<bean id="orderHandOutAction"
		class="com.huateng.univer.seller.sellorder.OrderHandOutAction"
		scope="prototype">
	</bean>
	
	<!-- 订单配送确定 -->
	
	<bean id="orderSendConfirmAction"
		class="com.huateng.univer.seller.sellorder.OrderSendConfirmAction"
		scope="prototype">
	</bean>
	
	<!-- 订单立即充值 -->
	<bean id="orderImmediatelyCreditAction"
		class="com.huateng.univer.seller.creditorder.OrderImmediatelyCreditAction"
		scope="prototype">
	</bean>
	
	
	<!-- 销售记名卡订单 -->
	<bean id="sellOrderSignAction"
		class="com.huateng.univer.seller.sellorder.SellOrderSignAction"
		scope="prototype">
	</bean>
	
	<!-- 销售匿名卡订单 -->
	<bean id="sellOrderUnsignAction"
		class="com.huateng.univer.seller.sellorder.SellOrderUnsignAction"
		scope="prototype">
	</bean>
	<!-- 销售记名卡订单(散户) -->
	<bean id="sellOrderRetailSignAction"
		class="com.huateng.univer.seller.sellorderretail.SellOrderRetailSignAction"
		scope="prototype">
	</bean>
	
	<!-- 销售不记名卡订单 （散户）-->
	<bean id="sellOrderRetailUnsignAction"
		class="com.huateng.univer.seller.sellorderretail.SellOrderRetailUnsignAction"
		scope="prototype">
	</bean>
	
	<!-- 充值订单 -->
	
	<bean id="creditOrderAction"
		class="com.huateng.univer.seller.creditorder.CreditOrderAction"
		scope="prototype">
	</bean>
	
	
	<!-- 换卡订单 -->
	<bean id="changeCardOrderAction"
		class="com.huateng.univer.seller.sellorder.ChangeCardOrderAction"
		scope="prototype">
	</bean>
	
	<!-- 赎回订单 -->
	<bean id="ransomOrderAction"
		class="com.huateng.univer.seller.sellorder.RansomOrderAction"
		scope="prototype">
	</bean>
	
	
	<!-- 赎回订单验卡 -->
	<bean id="randsomCheckAction"
		class="com.huateng.univer.seller.sellorder.RandsomCheckAction"
		scope="prototype">
	</bean>
	
	
	<!-- 制卡订单查询 -->
	<bean id="stockOrderQueryAction" 
		class="com.huateng.univer.issuer.order.StockOrderQueryAction"
		scope="prototype" />
	
	<!-- 制卡订单录入 -->
	<bean id="stockOrderInputAction" 
		class="com.huateng.univer.issuer.order.StockOrderInputAction"
		scope="prototype" />

	<!-- 制卡订单确定 -->
	<bean id="stockOrderConfirmAction" 
		class="com.huateng.univer.issuer.order.StockOrderConfirmAction"
		scope="prototype" />
		
	<!-- 制卡订单制卡文件下载 -->
	<bean id="stockOrderMakeCardDownAction" 
		class="com.huateng.univer.issuer.order.StockOrderMakeCardDownAction"
		scope="prototype" />	
	
	<!-- 制卡订单制卡文件生成 -->
	<bean id="stockOrderMakeCardMakeAction" 
		class="com.huateng.univer.issuer.order.StockOrderMakeCardMakeAction"
		scope="prototype" />
		
	<!-- 制卡订单准备 -->
	<bean id="stockOrderReadyAction" 
		class="com.huateng.univer.issuer.order.StockOrderReadyAction"
		scope="prototype" />
	
	<!-- 制卡订单接收 -->
	<bean id="stockOrderAcceptAction" 
		class="com.huateng.univer.issuer.order.StockOrderAcceptAction"
		scope="prototype" />
		
	<!-- 制卡订单配送 -->
	<bean id="stockOrderSendAction" 
		class="com.huateng.univer.issuer.order.StockOrderSendAction"
		scope="prototype" />
		
	<!-- 采购记名订单 -->	
	<bean id="buyOrderSignAction" 
		class="com.huateng.univer.seller.buyorder.BuyOrderSignAction"
		scope="prototype" />
	
	<!-- 采购匿名订单 -->	
	<bean id="buyOrderUnSignAction" 
		class="com.huateng.univer.seller.buyorder.BuyOrderUnSignAction"
		scope="prototype" />
	
	<!-- 订单付款确认 -->
	
	<bean id="orderPaymentAction" 
		class="com.huateng.univer.seller.sellorder.OrderPaymentAction"
		scope="prototype" />
		
	<!-- 代发卡 by Yifeng.Shi @ 2011-03-14 -->
	
	<bean id="loyaltyOrderAction" 
		class="com.huateng.univer.issuer.order.LoyaltyOrderAction"
		scope="prototype" />	
		
	<!-- 分页测试 -->
	
	<bean id="pageAction" 
		class="com.huateng.univer.page.PageAction"
		scope="prototype" />
	
	
	<!-- 批量处理 -->
	<bean id="batchFileAction" 
		class="com.huateng.univer.batchfile.BatchFileAction"
		scope="prototype" />
		
	<!-- 库存调拨订单录入 -->
	<bean id="stockTransferOrderInputAction" 
		class="com.huateng.univer.seller.stockTransferOrder.StockTransferOrderInputAction"
		scope="prototype" />
		
	<!-- 库存调拨订单出库-->
	<bean id="stockTransferOrderReadyAction" 
		class="com.huateng.univer.seller.stockTransferOrder.StockTransferOrderReadyAction"
		scope="prototype" />
		
	<!-- 库存调拨订单出库-->
	<bean id="stockTransferOrderAcceptAction" 
		class="com.huateng.univer.seller.stockTransferOrder.StockTransferOrderAcceptAction"
		scope="prototype" />
		
	<!-- 库存调拨订单查询-->
	<bean id="stockTransferOrderQueryAction" 
		class="com.huateng.univer.seller.stockTransferOrder.StockTransferOrderQueryAction"
		scope="prototype" />
	<!-- 卡信息管理查询-->
	<bean id="cardInformationQueryAction" 
		class="com.huateng.univer.cardinfor.CardInformation"
		scope="prototype" />
	<!-- 卡信息管理查询-->
	<bean id="cardInfoQueryAction" 
		class="com.huateng.univer.cardinfor.CardInfoQueryAction"
		scope="prototype" />
	<!-- 卡邮寄查询-->
	<bean id="cardMailQueryAction" 
		class="com.huateng.univer.cardmail.CardMail"
		scope="prototype" />
		
	<!-- 卡信息审核确定 -->
	<bean id="cardInformationAction"
		class="com.huateng.univer.cardinfor.CardInformationAction"
		scope="prototype" />
	
	<!-- 卡邮寄确定 -->
	<bean id="cardMailAction" 
		class="com.huateng.univer.cardmail.CardMailAction"
		scope="prototype" />
		
</beans>