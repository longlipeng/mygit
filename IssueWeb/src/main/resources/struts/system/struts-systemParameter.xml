<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="systemParameter" extends="struts-global"  namespace="/systemParameter">
		<global-results>
			<result name="list">/WEB-INF/pages/univer/system/sysparam/systemParameterList.jsp</result>
			<result name="view">/WEB-INF/pages/univer/system/sysparam/systemParameterView.jsp</result>
			<result name="edit">/WEB-INF/pages/univer/system/sysparam/systemParameterEdit.jsp</result>
		</global-results>
		
		<action name="listSystemParameter" class="systemParameterAction" method="inquery">
			<result name="input">/WEB-INF/pages/univer/system/sysparam/systemParameterList.jsp</result>
		</action>
		
		<action name="viewSystemParameter" class="systemParameterAction" method="load">
			<result name="input">/WEB-INF/pages/univer/system/sysparam/systemParameterList.jsp</result>
		</action>
		
		<action name="editSystemParameter" class="systemParameterAction" method="edit">
			<result name="input">/WEB-INF/pages/univer/system/sysparam/systemParameterList.jsp</result>
		</action>
		
		<action name="updateSystemParameter" class="systemParameterAction" method="update">
			<interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/system/sysparam/systemParameterList.jsp</result>
			<result name="input">/WEB-INF/pages/univer/system/sysparam/systemParameterEdit.jsp</result>
		</action>
	</package>
</struts>
