<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="dictInfo" extends="struts-global"  namespace="/dictInfo">
	<global-results>
		<result name="list">/WEB-INF/pages/univer/system/dictinfo/dictInfoList.jsp</result>
		<result name="view">/WEB-INF/pages/univer/system/dictinfo/dictInfoView.jsp</result>
	</global-results>
		<action name="inqueryDictInfo" class="dictInfoAction" method="inquery">
			<result name="input">/WEB-INF/pages/univer/system/dictinfo/dictInfoList.jsp</result>
		</action>
		
		<action name="redirectAddDictInfo" class="dictInfoAction" method="redirectAdd">
			<result name="add">/WEB-INF/pages/univer/system/dictinfo/dictInfoAdd.jsp</result>
		</action>
		<action name="addDictInfo" class="dictInfoAction" method="add">
			<interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/system/dictinfo/dictInfoList.jsp</result>
			<result name="input">/WEB-INF/pages/univer/system/dictinfo/dictInfoAdd.jsp</result>
		</action>
		<action name="viewDictInfo" class="dictInfoAction" method="view">
			<result name="input">/WEB-INF/pages/univer/system/dictinfo/dictInfoList.jsp</result>
			<result name="shopView">/WEB-INF/pages/univer/system/dictinfo/shopDictInfoView.jsp</result>
			<result name="cityView">/WEB-INF/pages/univer/system/dictinfo/cityDictInfoView.jsp</result>
		</action>
		
		<action name="loadDictInfo" class="dictInfoAction" method="load">
			<result name="input">/WEB-INF/pages/univer/system/dictinfo/dictInfoList.jsp</result>
			<result name="edit">/WEB-INF/pages/univer/system/dictinfo/dictInfoEdit.jsp</result>
			<!--<result name="shopEdit">/WEB-INF/pages/univer/system/dictinfo/shopDictInfoEdit.jsp</result>
			<result name="cityEdit">/WEB-INF/pages/univer/system/dictinfo/cityDictInfoEdit.jsp</result>
		--></action>
		
		<action name="editDictInfo" class="dictInfoAction" method="edit">
			<interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/system/dictinfo/dictInfoList.jsp</result>
			<result name="input">/WEB-INF/pages/univer/system/dictinfo/dictInfoEdit.jsp</result>
		</action>
		
		<action name="delDictInfo" class="dictInfoAction" method="del">
		</action>
		
	</package>
</struts>
