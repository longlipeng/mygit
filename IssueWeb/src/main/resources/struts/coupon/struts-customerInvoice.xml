<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="customerInvoice" extends="struts-global"
		namespace="/customerInvoice">

		<global-results>
			<result name="list">/WEB-INF/pages/univer/coupon/customerInvoiceList.jsp
			</result>
			<result name="edit">/WEB-INF/pages/univer/coupon/customerInvoiceEdit.jsp
			</result>
			<result name="view">/WEB-INF/pages/univer/coupon/customerInvoiceView.jsp
			</result>
			<result name="choice">/WEB-INF/pages/univer/coupon/invoiceItemChoice.jsp
			</result>
		</global-results>

		<action name="inquiry" class="customerInvoiceMaintainAction"
			method="list">
		</action>

		<action name="view" class="customerInvoiceMaintainAction"
			method="query">
			<result name="input">/WEB-INF/pages/univer/coupon/customerInvoiceView.jsp
			</result>
		</action>

		<action name="tomodify" class="customerInvoiceMaintainAction"
			method="query">
			<result name="input">/WEB-INF/pages/univer/coupon/customerInvoiceEdit.jsp
			</result>
		</action>

		<action name="modify" class="customerInvoiceMaintainAction"
			method="edit">
			<result name="input">/WEB-INF/pages/univer/coupon/customerInvoiceEdit.jsp
			</result>
		</action>

		<action name="pickItem" class="customerInvoiceMaintainAction"
			method="choose">
		</action>

	</package>
</struts>
