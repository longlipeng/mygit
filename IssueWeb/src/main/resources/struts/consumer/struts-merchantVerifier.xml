			</result>
		</action>
		<action name="edit" class="merchantVerifierAction" method="edit">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierList.jsp
			</result>
		</action>
		<action name="view" class="merchantVerifierAction" method="view">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierList.jsp
			</result>
		</action>
		<action name="inquiry" class="merchantVerifierAction" method="inquiry">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierList.jsp
			</result>
		</action>

		<action name="update" class="merchantVerifierAction" method="update">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierEdit.jsp
			</result>
		</action>
		<action name="addContact" class="merchantVerifierAction"
			method="addContact">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierEdit.jsp
			</result>
		</action>
		<action name="insertContact" class="merchantVerifierAction"
			method="insertContact">
			<result name="input">/WEB-INF/pages/univer/entitybaseinfo/contact/contactManagementAdd.jsp
			</result>
		</action>
		<action name="editContact" class="merchantVerifierAction"
			method="editContact">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierEdit.jsp
			</result>
		</action>
		<action name="updateContact" class="merchantVerifierAction"
			method="updateContact">
			<result name="input">/WEB-INF/pages/univer/entitybaseinfo/contact/contactManagementEdit.jsp
			</result>
		</action>
		<action name="viewContact" class="merchantVerifierAction"
			method="viewContact">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierList.jsp
			</result>
		</action>
		<action name="deleteContact" class="merchantVerifierAction"
			method="deleteContact">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierEdit.jsp
			</result>
		</action>
		<action name="addBank" class="merchantVerifierAction" method="addBank">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierEdit.jsp
			</result>
		</action>
		<action name="deleteBank" class="merchantVerifierAction"
			method="deleteBank">
			<result name="input">/WEB-INF/pages/univer/consumer/merchantVerifier/merchantVerifierEdit.jsp
			</result>
		</action>
		<action name="insertBank" class="merchantVerifierAction"
			method="insertBank">
			<result name="input">/WEB-INF/pages/univer/entitybaseinfo/bank/bankManagementAdd.jsp
			</result>
		</action>
		
        <action name="updateBank" class="merchantVerifierAction" method="updateBank">
          <result name="input">/WEB-INF/pages/univer/entitybaseinfo/bank/edit.jsp</result>
        </action>
        <action name="editBank" class="merchantVerifierAction" method="editBank">
          <result name="input">/WEB-INF/pages/merchant/merchantManagementList.jsp</result>
        </action>
