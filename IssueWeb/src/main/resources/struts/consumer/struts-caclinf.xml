<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="caclInf" extends="struts-global"  namespace="/caclInf">
       <global-results>
            <result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
        </global-results>
      <action name="AddCaclInf" class="caclInfAction" method="AddCaclInf">
      <interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleAdd.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="editConsumerContract" type="redirectAction">
          <param name="actionName">edit</param>
           <param name="namespace">/mchntContractManagement</param>
            <param name="consumerContractId">%{consumerContractId}</param>
         </result>
      </action>
      <action name="listCaclInf" class="caclInfAction" method="listCaclInf">
      <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      </action>
      <action name="view" class="caclInfAction" method="view">
       <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="view">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleView.jsp</result>
      	<result name="EditView">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleEdit.jsp</result>
      </action>
      
        <action name="viewModalDialog" class="caclInfAction" method="view">
       <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="view">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleViewModalDialog.jsp</result>
      	<result name="EditView">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleEdit.jsp</result>
      </action>
      
      <action name="update" class="caclInfAction" method="update">
      	<interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
       <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleEdit.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
	    <result name="edit">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleEdit.jsp</result>
	    <result name="editConsumerContract" type="redirectAction">
          <param name="actionName">edit</param>
           <param name="namespace">/mchntContractManagement</param>
            <param name="consumerContractId">%{consumerContractId}</param>
         </result>
      </action>
      <action name="add" class="caclInfAction" method="add">
       <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="add">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleAdd.jsp</result>
      </action>
      <action name="addInf" class="caclInfAction" method="addInf">
      <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      	<result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>     	
      </action>
    </package>
</struts>
