<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="stockCard" extends="struts-global"  namespace="/stockCard">
       <global-results>
            <result name="list">/WEB-INF/pages/univer/stockcard/StockCardList.jsp</result>
        </global-results>
        
       <action name="query" class="stockCardAction" method="query">
          <result name="input">/WEB-INF/pages/univer/stockcard/StockCardList.jsp</result>
      </action>
      <action name="cardListView" class="stockCardAction" method="cardListView">
           <result name="cardNo">/WEB-INF/pages/univer/stockcard/StockCardNoList.jsp</result>
      </action>
      <action name="view" class="stockCardAction" method="view">
          <result name="input">/WEB-INF/pages/univer/stockcard/StockCardList.jsp</result>
          <result name="view">/WEB-INF/pages/univer/stockcard/StockCardView.jsp</result>
      </action>
      
    </package>
    <package name="stockAllocate" extends="struts-global"  >
    <action name="stockAllocateAction" class="stockAllocateAction">
      	<result name="stockAllocateList">/WEB-INF/pages/univer/stockcard/stockAllocateList.jsp</result>
      	<result name="stockAllocateAdd">/WEB-INF/pages/univer/stockcard/stockAllocateAdd.jsp</result>
      	<result name="allocateView">/WEB-INF/pages/univer/stockcard/stockAllocateView.jsp</result>
      	<result name="readyOrderList">/WEB-INF/pages/univer/stockcard/StockAllocateReadyOrderList.jsp</result>
      	<result name="cardList">/WEB-INF/pages/univer/stockcard/stockAllocateReady.jsp</result>
      	<result name="successList">/WEB-INF/pages/univer/seller/order/orderinput/sellorder/unsignorder/orderListSuccess.jsp</result>
      </action>
       <action name="updateStockState" class="stockCardAction" method="updateStockState">
           <result name="cardNo">/WEB-INF/pages/univer/stockcard/StockCardNoList.jsp</result>
      </action>
      
    </package>
</struts>
