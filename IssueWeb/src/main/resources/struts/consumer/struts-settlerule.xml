<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="settleRule" extends="struts-global"  namespace="/settleRule">
       <global-results>
            <result name="list">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
        </global-results>
        
      <action name="insert" class="settlePeriodRuleAction" method="insert">
      	  <interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
          <result name="input">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleAdd.jsp</result>
      </action>
      <action name="add" class="settlePeriodRuleAction" method="add">
          <result name="add">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleAdd.jsp</result>
      </action>
       <action name="query" class="settlePeriodRuleAction" method="query">
          <result name="input">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
      </action>
      <action name="enable" class="settlePeriodRuleAction" method="enable">
          <result name="input">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
      </action>
      <action name="editInit" class="settlePeriodRuleAction" method="editInit">
          <result name="editInit">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleEdit.jsp</result>
      </action>
      <action name="update" class="settlePeriodRuleAction" method="update">
      		 <interceptor-ref name="token" />
         	<interceptor-ref name="defaultStack" />
        	<result name="invalid.token">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
          <result name="input">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleEdit.jsp</result>
      </action>
      <action name="delete" class="settlePeriodRuleAction" method="delete">
          <result name="input">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
      </action>
      <action name="view" class="settlePeriodRuleAction" method="view">
          <result name="input">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleList.jsp</result>
          <result name="view">/WEB-INF/pages/univer/settleperiodrule/SettlePeriodRuleView.jsp</result>
      </action>
    </package>
</struts>
