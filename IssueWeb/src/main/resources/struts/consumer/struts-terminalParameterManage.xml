<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="terminalParameterManage" extends="struts-global"  namespace="/terParameter">
		<action name="inqueryPrm" class="terParameterAction" method="inquery">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
        </action>
     	
     	<action name="redirectAddPrm" class="terParameterAction" method="redirectAdd">
       		<result name="add">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterAdd.jsp</result>
        </action>
        
        
        <action name="addPrm" class="terParameterAction" method="add">
        	<result name="input">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterAdd.jsp</result>
       		<result name="add">/WEB-INF/pages/terminalManagement/posSystem/sysParameterAdd.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
        </action>
        
        <action name="viewPrm" class="terParameterAction" method="view">
        	<result name="view">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterView.jsp</result>
       		<result name="failed">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
        </action>
        
         <action name="loadPrm" class="terParameterAction" method="load">
        	<result name="edit">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterEdit.jsp</result>
       		<result name="failed">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
        </action>
        
         <action name="updatePrm" class="terParameterAction" method="update">
        	<result name="failed">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterEdit.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterEdit.jsp</result>
        </action>
        <action name="redirectAddkaBin" class="terParameterAction" method="redirectAddkaBin">
       		<result name="kaBinAdd">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterAddkaBin.jsp</result>
        </action>
        
         <action name="redirectAddIcCard" class="terParameterAction" method="redirectAddIcCard">
       		<result name="IcCardAdd">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterAddIcCard.jsp</result>
        </action>
        
         <action name="addkaBin" class="terParameterAction" method="addkaBin">
        	<result name="input">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterAddkaBin.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
        </action>
        
         <action name="addIcCard" class="terParameterAction" method="addIcCard">
        	<result name="input">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterAddIcCard.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posparamenter/sysParameterList.jsp</result>
        </action>
        
          <action name="comPrm" class="terParameterAction" method="remainingComPara">       	
        </action>
        
	</package>
</struts>
