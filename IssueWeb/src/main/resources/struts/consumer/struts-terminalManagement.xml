<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="terminalManagement" extends="struts-global" >
		<action name="initTerminal" class="terminalManagementAction" method="init">
       		<result name="list">/WEB-INF/pages/terminalManagement/posInf/posInfList.jsp</result>
        </action>
          <action name="inqueryTerinalManagement" class="terminalManagementAction" method="inquery">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
        </action>
        <action name="inqueryOuterposOrPos" class="terminalManagementAction" method="inqueryOuterposOrPos">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
        </action>
          
        <action name="redircetAddposInf" class="terminalManagementAction" method="redircetAdd">
       		<result name="add">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfAdd.jsp</result>
       		
        </action>
         <action name="addOuterPosInf" class="terminalManagementAction" method="addOuterPosInf">
       		<result name="outerPosadd">/WEB-INF/pages/univer/consumer/pos/posinfo/outerPosInfAdd.jsp</result>
        </action>
         <action name="insertOuterPosInf" class="terminalManagementAction" method="insertOuterPosInf">
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/outerPosInfAdd.jsp</result>
