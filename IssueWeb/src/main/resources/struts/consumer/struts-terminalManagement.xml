        </action>
          
        <action name="addPosBrandInf" class="terminalManagementAction" method="addPosBrandInf">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfAdd.jsp</result>
       		<result name="failed">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfAdd.jsp</result>
        </action>
        
        
         <action name="loadPosBrandInf" class="terminalManagementAction" method="loadPosBrandInf">
       		<result name="edit">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfEdit.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfList.jsp</result>
        </action>
        
         <action name="updatePosBrandInf" class="terminalManagementAction" method="updatePosBrandInf">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posbrandinfo/posBrandInfEdit.jsp</result>
        </action>
        
           
        <action name="inquiryShop" class="terminalManagementAction" method="inquiry">
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/chooseShop.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/chooseShop.jsp</result>
        </action>
        
        <action name="choosePosBrandInf" class="terminalManagementAction" method="choosePosBrandInf">
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/chooseposBrandInf.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/chooseposBrandInf.jsp</result>
        </action>
        
        <action name="updateParameter" class="terminalManagementAction" method="updateParameter">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
        </action>
        
        
	</package>
</struts>
