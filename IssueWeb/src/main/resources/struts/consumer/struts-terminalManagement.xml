        </action>
        
        <action name="addposInf" class="terminalManagementAction" method="addposInf">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="add">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfAdd.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfAdd.jsp</result>
        </action>
        
         <action name="delposInf" class="terminalManagementAction" method="delposInf">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
        </action>
       
        
        <action name="viewposInf" class="terminalManagementAction" method="viewposInf">
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="outerPosView">/WEB-INF/pages/univer/consumer/pos/posinfo/outerPosView.jsp</result>
       		<result name="posInfView">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfView.jsp</result>
        </action>
        
        <action name="loadposInf" class="terminalManagementAction" method="loadposInf">
       		<result name="edit">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfEdit.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfEdit.jsp</result>
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="editError">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfEdit.jsp</result>
       		<result name="editSucc">/WEB-INF/pages/univer/consumer/pos/posinfo/outerPosInfEdit.jsp</result>
        </action>
        
        
        <action name="updateposInf" class="terminalManagementAction" method="updateposInf">
       		<result name="list">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfList.jsp</result>
       		<result name="input">/WEB-INF/pages/univer/consumer/pos/posinfo/posInfEdit.jsp</result>
       		<result name="fail">/WEB-INF/pages/univer/consumer/pos/posinfo/outerPosInfEdit.jsp</result>
       		
        </action>
         
        
         <action name="selectMerchant" class="terminalManagementAction" method="selectMerchant">
