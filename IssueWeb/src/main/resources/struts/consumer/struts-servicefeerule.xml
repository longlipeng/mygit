<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="serviceFeeRule" extends="struts-global"  namespace="/serviceFeeRule">
       <global-results>
            <result name="list">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
        </global-results>
       <action name="query" class="serviceFeeRuleAction" method="query">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      </action>
      <action name="add" class="serviceFeeRuleAction" method="add">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
          <result name="add">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleAdd.jsp</result>
      </action>
      <action name="insert" class="serviceFeeRuleAction" method="insert">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleAdd.jsp</result>
      </action>
      <action name="editInit" class="serviceFeeRuleAction" method="editInit">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
          <result name="editInit">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleEdit.jsp</result>
      </action>
       <action name="update" class="serviceFeeRuleAction" method="update">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleEdit.jsp</result>
      </action>
      <action name="view" class="serviceFeeRuleAction" method="view">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
          <result name="view">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleView.jsp</result>
      </action>
       <action name="enable" class="serviceFeeRuleAction" method="enable">
          <result name="input">/WEB-INF/pages/univer/servicefeerule/ServiceFeeRuleList.jsp</result>
      </action>
    </package>
</struts>
