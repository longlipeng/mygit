<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="cardCompany" extends="struts-global"  namespace="/cardCompany">
        <global-results>
            <result name="list">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
            <result name="view">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyView.jsp</result>
            <result name="add">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyAdd.jsp</result>
            <result name="edit">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyEdit.jsp</result>
            <result name="modifyState">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyModify.jsp</result>
        </global-results>
       
       	<action name="inquery" class="cardCompanyAction" method="list">
       		<result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
       	</action>
        <action name="view" class="cardCompanyAction" method="view">
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
        </action>
        <action name="add" class="cardCompanyAction" method="add">
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
        </action>
        <action name="insert" class="cardCompanyAction" method="insert">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result> 
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyAdd.jsp</result>
        </action>
        <action name="edit" class="cardCompanyAction" method="edit">
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
        </action>
        <action name="update" class="cardCompanyAction" method="update">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyEdit.jsp</result>
        </action>

        <action name="modifyState" class="cardCompanyAction" method="modifyState">
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result>
        </action>
        <action name="updateState" class="cardCompanyAction" method="updateState">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyList.jsp</result> 
            <result name="input">/WEB-INF/pages/univer/issuer/cardcompany/cardCompanyModify.jsp</result>
        </action>
    </package>
</struts>
