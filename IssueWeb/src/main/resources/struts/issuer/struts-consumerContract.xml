<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>                                                              
    <package name="consumerContract" extends="struts-global"  namespace="/consumerContract">
        <global-results>
           <result name="list">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractList.jsp</result>
           <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractList.jsp</result>
           <result name="addAcctypeContract">/WEB-INF/pages/univer/consumer/acctypecontract/AccTypeContractAdd.jsp</result>
            <result name="editAcctypeContract">/WEB-INF/pages/univer/consumer/acctypecontract/AccTypeContractContractEdit.jsp</result>
            <result name="viewAcctypeContract">/WEB-INF/pages/acctypecontract/acctypeContractManagementDetail.jsp</result>
             <result name="blank">/WEB-INF/pages/univer/entitybaseinfo/blank.jsp</result>
        </global-results>
                
        <action name="inquery" class="consumerContractAction" method="query">
         <result name="list">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractList.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractList.jsp</result>
        </action>
        
        <action name="reAdd" class="consumerContractAction" method="reAdd">
         <result name="readd">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractAdd.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractList.jsp</result>
        </action>
        
        <action name="insert" class="consumerContractAction" method="insertConsumer">
         <result name="edit">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractAdd.jsp</result>
        </action>
        
        <action name="edit" class="consumerContractAction" method="editConsumerContract">
         <result name="edit">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractAdd.jsp</result>
        </action>
        
        <action name="view" class="consumerContractAction" method="viewCinsumerContract">
         <result name="edit">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractView.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractList.jsp</result>
        </action>
        
        <action name="update" class="consumerContractAction" method="updateCinsumerContract">
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
        </action>
        
         <action name="addAcctypeContract" class="consumerContractAction" method="addAcctypeContract">
         <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
        </action>
        <action name="insertAcctypeContract" class="consumerContractAction" method="insertAcctypeContract">
          <result name="input">/WEB-INF/pages/univer/consumer/acctypecontract/AccTypeContractAdd.jsp</result>
        </action>
        <action name="editAcctypeContract" class="consumerContractAction" method="editAcctypeContract">
          <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
        </action>
        <action name="updateAcctypeContract" class="consumerContractAction" method="updateAcctypeContract">
          <result name="input">/WEB-INF/pages/univer/consumer/acctypecontract/AccTypeContractContractEdit.jsp</result>
        </action>
        <action name="deleteAcctypeContract" class="consumerContractAction" method="deleteAcctypeContract">
          <result name="input">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
           <result name="edit">/WEB-INF/pages/univer/issuer/consumercontract/consumerContractEdit.jsp</result>
        </action>
    </package>
</struts>
