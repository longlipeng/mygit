<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>                                                              
    <package name="package" extends="struts-global">
        <global-results>
          <result name="list">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
          <result name="add">/WEB-INF/pages/univer/issuer/package/packageAdd.jsp</result>
          <result name="edit">/WEB-INF/pages/univer/issuer/package/packageEdit.jsp</result>
          <result name="input">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
        </global-results>
                
        <action name="packageInquery" class="packageAction" method="query"></action>
        <action name="packageInsert" class="packageAction" method="redirectAdd"></action>
        <action name="insert" class="packageAction" method="insert">
        <interceptor-ref name="token"></interceptor-ref>
         <interceptor-ref name="defaultStack" />
        <result name="invalid.token">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/package/packageAdd.jsp</result>
        </action>
        <action name="packageLoad" class="packageAction" method="load"></action>
        <action name="update" class="packageAction" method="update">
         <interceptor-ref name="token"></interceptor-ref>
         <interceptor-ref name="defaultStack" />
        <result name="invalid.token">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/package/packageEdit.jsp</result>
        </action>
        <action name="delete" class="packageAction" method="delete">
         <interceptor-ref name="token" />
         <interceptor-ref name="defaultStack" />
        <result name="invalid.token">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
        </action>
        <action name="view" class="packageAction" method="view">
         <result name="view">/WEB-INF/pages/univer/issuer/package/packageView.jsp</result>
         <result name="input">/WEB-INF/pages/univer/issuer/package/packageList.jsp</result>
        </action>
        
    </package>
</struts>
