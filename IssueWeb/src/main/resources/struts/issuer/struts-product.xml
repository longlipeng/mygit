<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="product" extends="struts-global">
		<global-results>
			<result name="list">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</global-results>

		<action name="productInquery" class="productAction" method="init">
			<result name="list">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
		</action>

		<action name="productReAdd" class="productAction" method="reAdd">
			<result name="readd">/WEB-INF/pages/univer/issuer/product/productAdd.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
		</action>
		<action name="insertProduct" class="productAction" method="insertProduct">
			<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productAdd.jsp
			</result>
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="productAccountInquery" class="productAction"
			method="accountInquery">
			<result name="input">/WEB-INF/pages/univer/issuer/product/accountAddList.jsp
			</result>
			<result name="accountAdd">/WEB-INF/pages/univer/issuer/product/accountAddList.jsp
			</result>
		</action>
		<action name="accountAdd" class="productAction" method="accountAdd">
			<result name="input">/WEB-INF/pages/univer/issuer/product/accountAddList.jsp
			</result>
			<result name="edit">/WEB-INF/pages/univer/issuer/product/accountAddList.jsp
			</result>
		</action>
		<action name="load" class="productAction" method="load">
			<result name="list">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
		</action>

		<action name="accountEdit" class="productAction" method="accountEdit">
			<result name="acctypeEdit">
				/WEB-INF/pages/univer/issuer/product/accountEdit.jsp</result>
		</action>
		<action name="prodAccounteEdit" class="productAction" method="prodAccounteEdit">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/accountEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/accountEdit.jsp
			</result>
		</action>
		<action name="accountDel" class="productAction" method="accountDel">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="serviceQuery" class="productAction" method="serviceQuery">
			<result name="accountAdd">/WEB-INF/pages/univer/issuer/product/accountAddList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/accountAddList.jsp
			</result>
		</action>
		<action name="updateProduct" class="productAction" method="update">
			<!-- <interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result> -->
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="cardLayoutList" class="productAction" method="cardLayoutList">
			<result name="cardLayoutList">
				/WEB-INF/pages/univer/issuer/product/cardLayoutAddList.jsp</result>
		</action>
		<action name="getCardLayoutImg" class="productAction" method="getCardLayoutImg"></action>
		<action name="cardLayoutAdd" class="productAction" method="cardLayoutAdd">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="productCardLayoutDel" class="productAction"
			method="productCardLayoutDel">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="faceValueList" class="productAction" method="faceValueList">
			<result name="faceValueList">/WEB-INF/pages/univer/issuer/product/productFaceValueAdd.jsp
			</result>
		</action>
		<action name="faceValueAdd" class="productAction" method="faceValueAdd">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="prodFaceValueDel" class="productAction" method="prodFaceValueDel">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="productPackageList" class="productAction" method="productPackageList">
			<result name="packageList">/WEB-INF/pages/univer/issuer/product/productPackageList.jsp
			</result>
		</action>
		<action name="packageAdd" class="productAction" method="packageAdd">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="packageDel" class="productAction" method="packageDel">
			<result name="edit">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>
		<action name="productPackageList1" class="productAction"
			method="productPackageList1">
			<result name="packageList">/WEB-INF/pages/univer/issuer/product/productPackageList.jsp
			</result>
		</action>
		<action name="productDelete" class="productAction" method="productDelete">
			<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
			<result name="list">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
		</action>
		<action name="productView" class="productAction" method="view">
			<result name="view">/WEB-INF/pages/univer/issuer/product/productView.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/product/productList.jsp
			</result>
		</action>

		<action name="choiceCardBin" class="productAction" method="choiceCardBin">
			<result name="choiceCardBin">/WEB-INF/pages/univer/issuer/product/cardBinChoice.jsp
			</result>
		</action>

		<action name="insertCardBinForProduct" class="productAction"
			method="insertCardBin">
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>

		<action name="modifyCardBinDefault" class="productAction"
			method="modifyCardBinDefault">
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>

		<action name="deleteCardBin" class="productAction" method="deleteCardBin">
			<result name="input">/WEB-INF/pages/univer/issuer/product/productEdit.jsp
			</result>
		</action>

	</package>
</struts>
