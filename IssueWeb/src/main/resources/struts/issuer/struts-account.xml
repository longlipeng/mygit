<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="account" extends="struts-global">
		<global-results>
			<result name="list">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
		</global-results>

		<action name="accountInquery" class="accountAction" method="init">
			<result name="list">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
		</action>
		<action name="accountInsert" class="accountAction" method="reload">
			<result name="reAdd">/WEB-INF/pages/univer/issuer/account/accountAdd.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/account/accountAdd.jsp
			</result>
		</action>

		<action name="insertAccount" class="accountAction" method="insertAccount">
			<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/account/accountAdd.jsp
			</result>
		</action>

		<action name="accountLoad" class="accountAction" method="accountLoad">
			<result name="edit">/WEB-INF/pages/univer/issuer/account/accountEdit.jsp
			</result>
		</action>
		<action name="updateAccount" class="accountAction" method="updateAccount">
			<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/account/accountEdit.jsp
			</result>
		</action>
		<action name="deleteAccount" class="accountAction" method="delete">
			<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
		</action>

		<action name="accountView" class="accountAction" method="accountView">
			<result name="view">/WEB-INF/pages/univer/issuer/account/accountView.jsp
			</result>
			<result name="input">/WEB-INF/pages/univer/issuer/account/accountList.jsp
			</result>
		</action>

	</package>
</struts>
