<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="cardholder" extends="struts-global" namespace="/cardholder">
        <global-results>
        	<result name="checkPerView">/WEB-INF/pages/univer/seller/cardholder/checkCardholderView.jsp</result>
        	<result name="checkCusView">/WEB-INF/pages/univer/seller/cardholder/checkCusCardholderView.jsp</result>
        	<result name="cusView">/WEB-INF/pages/univer/seller/cardholder/cusCardholderView.jsp</result>
        	<result name="editCus">/WEB-INF/pages/univer/seller/cardholder/cusCardholderEdit.jsp</result>
        	<result name="addCustomer">/WEB-INF/pages/univer/seller/cardholder/cardholderCustomerAdd.jsp</result>
            <result name="list">/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>                        
            <result name="view">/WEB-INF/pages/univer/seller/cardholder/cardholderView.jsp</result>
            <result name="add">/WEB-INF/pages/univer/seller/cardholder/cardholderAdd.jsp</result>
            <result name="edit">/WEB-INF/pages/univer/seller/cardholder/cardholderEdit.jsp</result>
            <result name="modifyCust">/WEB-INF/pages/univer/seller/cardholder/cardholderModifyCust.jsp</result>
        </global-results>
        
        <action name="inquery" class="cardholderAction" method="list">
	        <result name="cardManageQuery" type="redirectAction">
		        <param name="actionName">cardQuery</param>
		        <param name="namespace">/cardManage</param>
		        <param name="cardManagementDTO.transferOutCard">${cardManagementDTO.transferOutCard}</param>        
	        </result>
	        <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
        </action>
        
        <action name="inqueryBycheck" class="cardholderAction" method="checkList">
	        <result name="cardManageQuery" type="redirectAction">
		        <param name="actionName">cardQuery</param>
		        <param name="namespace">/cardManage</param>
		        <param name="cardManagementDTO.transferOutCard">${cardManagementDTO.transferOutCard}</param>
	        </result>
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        	<result name="check">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        </action>
        <action name="checkView" class="cardholderAction" method="checkView">
        	<result name="checkView">/WEB-INF/pages/univer/seller/cardholder/checkCardholderView.jsp</result>
        </action>
        <action name="checkUpdate" class="cardholderAction" method="checkCardholder">
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/checkCardholderView.jsp</result>
            <result name="check">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        </action>
        <action name="view" class="cardholderAction" method="view">
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderView.jsp</result>
        </action>
        
        <action name="getImageInfo" class="cardholderAction" method="getImageInfo">
        </action>
              
        <action name="add" class="cardholderAction" method="add">
        	 <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderAdd.jsp</result>
        </action>
        
        <action name="insert" class="cardholderAction" method="insert">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderAdd.jsp</result>
        </action>
        
        <action name="edit" class="cardholderAction" method="edit">
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderEdit.jsp</result>
        </action>
        
        <action name="update" class="cardholderAction" method="update">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderEdit.jsp</result>
        </action>
       

        <action name="modifyCust" class="cardholderAction" method="modifyCust">
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderModifyCust.jsp</result>
        </action>
        
        <action name="updateCust" class="cardholderAction" method="updateCust">
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderModifyCust.jsp</result>
        </action>
        
        <action name="delete" class="cardholderAction" method="delete">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
        </action>
       	
       	<action name="import" class="cardholderAction">
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="success">/WEB-INF/pages/univer/seller/cardholder/cardholderImport.jsp</result>
        </action>
       	
       	<action name="checkFile" class="cardholderAction" method="checkFile">
            <result name="input" type="redirectAction">ajaxError</result>
        </action>
        <action name="importFile" class="cardholderAction" method="importFile">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderImport.jsp</result>
        </action>
        
        <action name="addInOrder" class="cardholderAction" method="addInOrder">
        	 <result name="addInOrder">/WEB-INF/pages/univer/seller/cardholder/cardholderAddInOrder.jsp</result>
        </action>
        
        <action name="insertInOrder" class="cardholderAction" method="insertInOrder">
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderAddInOrder.jsp</result>
        	<result name="addSucess">/WEB-INF/pages/univer/seller/order/orderinput/sellorder/signorder/orderListSuccess.jsp</result>
        </action>
        
        <action name="importFilePage" class="cardholderAction" method="importFilePage">
        	<result name="importFilePage">/WEB-INF/pages/univer/seller/cardholder/cardholderImportInOrder.jsp</result>
        </action>
        
        <action name="importFileInOrder" class="cardholderAction" method="importFileInOrder">
        	<result name="importSucess">/WEB-INF/pages/univer/seller/order/orderinput/sellorder/signorder/orderListSuccess.jsp</result>
        </action>
        <action name="checkIdNo" class="cardholderAction" method="checkIdNo">
        </action>
        <action name="checkOtherIdNo" class="cardholderAction" method="checkOtherIdNo">
        </action>
        
        <!--******************************新增区域************************************************* -->
        <!--查询持卡人列表 -->
        <action name="inqueryShow" class="cardholderAction" method="inqueryShow">
         <result name="cardManageQuery" type="redirectAction">
         <param name="actionName">cardQuery</param>
         <param name="namespace">/cardManage</param>
         <param name="cardManagementDTO.transferOutCard">${cardManagementDTO.transferOutCard}</param>       
        </result>
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
        </action>
        
         <!-- 添加个人持卡人-->
        <action name="saveCardholder" class="cardholderAction" method="saveCardholder">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderAdd.jsp</result>
        </action>
        
        <!-- 添加个人持卡人-->
        <action name="insertcardholder" class="cardholderAction" method="insertcardholder">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderAdd.jsp</result>
        </action>
        
        <!-- 添加企业持卡人 -->
        <action name="addCustomer" class="cardholderAction" method="addCustomer">
        	 <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderCustomerAdd.jsp</result>
        </action>
        <action name="insertcustomer" class="cardholderAction" method="insertcustomer">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderCustomerAdd.jsp</result>
        </action>
        
        <!--验证企业持卡人身份证信息 -->
        <action name="checkCusIdNo" class="cardholderAction" method="checkCusIdNo">
        </action>
        <action name="checkOtherCusIdNo" class="cardholderAction" method="checkOtherCusIdNo">
        </action>
        
        <!--编辑企业持卡人信息 -->
        <action name="editCus" class="cardholderAction" method="editCus">
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cusCardholderEdit.jsp</result>
        </action>
        <action name="updateCus" class="cardholderAction" method="updateCus">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/cusCardholderEdit.jsp</result>
        </action>
        
        <!--查看企业持卡人信息 -->
        <action name="cusView" class="cardholderAction" method="cusView">
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cusCardholderView.jsp</result>
        </action>
        
        <!--查看个人持卡人审核信息 -->
        <action name="checkPerView" class="cardholderAction" method="checkPerView">
        	<!-- <result name="checkPerView">/WEB-INF/pages/univer/seller/cardholder/checkCardholderView.jsp</result> -->
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        </action>
        
        <!--查看企业持卡人审核信息 -->
        <action name="checkCusView" class="cardholderAction" method="checkCusView">
        	<!-- <result name="checkCusView">/WEB-INF/pages/univer/seller/cardholder/checkCusCardholderView.jsp</result> -->
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        </action>
        
        <!--更新个人持卡人审核信息 -->
         <action name="checkPerUpdate" class="cardholderAction" method="checkPerUpdate">
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/checkCardholderView.jsp</result>
            <result name="checkPerUpdate">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        </action>
        
        <!--更新企业持卡人审核信息 -->
         <action name="checkCusUpdate" class="cardholderAction" method="checkCusUpdate">
            <result name="input">/WEB-INF/pages/univer/seller/cardholder/checkCusCardholderView.jsp</result>
            <result name="checkCusUpdate">/WEB-INF/pages/univer/seller/cardholder/checkCardholderList.jsp</result>
        </action>
        
        <!--校验企业证件号的唯一性 -->
        <action name="checkLicense" class="cardholderAction" method="checkLicense"></action>
        
        <!--删除企业持卡人信息 -->
        <action name="deleteCus" class="cardholderAction" method="deleteCus">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token" >/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderList.jsp</result>
        </action>
        
        <!--客户风险等级同步 -->
        <action name="queryRiskGRade" class="cardholderAction" method="queryRiskGRade">
	        <result name="input">/WEB-INF/pages/univer/seller/cardholder/riskGradeCrmList.jsp</result>
        	<result name="queryList">/WEB-INF/pages/univer/seller/cardholder/riskGradeCrmList.jsp</result>
        </action>
        
        <action name="riskGRadeCrmInfo" class="cardholderAction" method="riskGRadeCrmInfo">
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/riskGradeCrmList.jsp</result>
        	<result name="queryList">/WEB-INF/pages/univer/seller/cardholder/riskGradeCrmList.jsp</result>
        </action>
        
         <!-- 补录信息审核列表 -->
        <action name="informationAudit" class="cardholderAction" method="informationAudit" >
        	<result name="audit">
        		/WEB-INF/pages/univer/seller/cardholder/cardholderInformationAuditList.jsp
        	</result>
        </action>
         <!-- 补录信息审核页面 -->
        <action name="auditView" class="cardholderAction" method="auditView" >
        	<result name="auditView">
        		/WEB-INF/pages/univer/seller/cardholder/cardholderInfomationAuditView.jsp
        	</result>
        </action>
        
        <action name="auditCheck" class="cardholderAction" method="auditCheck" >
        	<result name="auditCheck">
        		/WEB-INF/pages/univer/seller/cardholder/cardholderInformationAuditList.jsp
        	</result>
        	<result name="input">/WEB-INF/pages/univer/seller/cardholder/cardholderInformationAuditList.jsp</result>
        </action>
        
         <!--******************************新增区域************************************************* -->  
        
    </package>
</struts>
