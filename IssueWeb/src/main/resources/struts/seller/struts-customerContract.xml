<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="customerContract" extends="struts-global"  namespace="/customerContract">
        <global-results>
            <result name="list">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
            <result name="productChoice">/WEB-INF/pages/univer/seller/customercontract/productChoice.jsp</result>
            <result name="settleRuleChoice">/WEB-INF/pages/univer/seller/customercontract/settleRuleChoice.jsp</result>
            <result name="addProductService">/WEB-INF/pages/univer/seller/customercontract/customerProductAdd.jsp</result>
            <result name="productView">/WEB-INF/pages/univer/seller/customercontract/customerProductAdd.jsp</result>
            <result name="view">/WEB-INF/pages/univer/seller/customercontract/customerContractView.jsp</result>
            <result name="add">/WEB-INF/pages/univer/seller/customercontract/customerContractAdd.jsp</result>
            <result name="edit">/WEB-INF/pages/univer/seller/customercontract/customerContractEdit.jsp</result>
            <result name="editProduct">/WEB-INF/pages/univer/seller/customercontract/customerProductEdit.jsp</result>
            <result name="editService">/WEB-INF/pages/univer/seller/customercontract/customerAccountEdit.jsp</result>
           	<result name="blank">/WEB-INF/pages/univer/seller/customercontract/blank.jsp</result>
           	
            <result name="masterPlateList">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateList.jsp</result>
            <result name="editMasterplate">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateEdit.jsp</result>
            <result name="viewMasterplate">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateView.jsp</result>
       	    <result name="blankMasterplate">/WEB-INF/pages/univer/seller/customercontractmasterplate/blank.jsp</result>
       	    <result name="addProductMasterplateService">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerProductAdd.jsp</result>
        </global-results>
        
        <action name="inquery" class="customerContractAction" method="list">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
        </action>
		
		<action name="productChoice" class="customerContractAction" method="productChoice">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
        </action>
        
		<action name="settleRuleChoice" class="customerContractAction" method="settleRuleChoice">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/settleRuleChoice.jsp</result>
        </action>
		
		<action name="productView" class="customerContractAction" method="productView">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
        </action>
        
		<action name="selectProductAjax" class="customerContractAction" method="selectAjax" />
        <action name="view" class="customerContractAction" method="view">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
        </action>
        
        <action name="add" class="customerContractAction" method="add">
        	 <result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
        </action>
        
        <action name="insert" class="customerContractAction" method="insert">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractAdd.jsp</result>
        </action>
        
        <action name="addProductService" class="customerContractAction" method="addProductService">
        	 <result name="input">/WEB-INF/pages/univer/seller/customercontract/customerProductAdd.jsp</result>
        </action>
        
        <action name="insertCustomerProductService" class="customerContractAction" method="insertCustomerProductService">
            <result name="input">/WEB-INF/pages/univer/seller/customercontract/customerProductAdd.jsp</result>
        </action>
        
        <action name="edit" class="customerContractAction" method="edit">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractEdit.jsp</result>
        </action>
                
        <action name="editProduct" class="customerContractAction" method="editProduct">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerProductEdit.jsp</result>
        </action>
        
        <action name="editService" class="customerContractAction" method="editService">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerAccountEdit.jsp</result>
        </action>
        
        <action name="update" class="customerContractAction" method="update">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractEdit.jsp</result>
        </action>
        
        <action name="updateProduct" class="customerContractAction" method="updateProduct">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerProductEdit.jsp</result>
        </action>
        
        <action name="updateService" class="customerContractAction" method="updateService">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerAccountEdit.jsp</result>
        </action>
        
        <action name="delete" class="customerContractAction" method="delete">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontract/customerContractList.jsp</result>
        </action>
        
         
        
        <action name="masterPlatelist" class="customerContractAction" method="masterPlatelist">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateList.jsp</result>
        </action>
        
        <action name="editMasterplate" class="customerContractAction" method="editMasterplate">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateEdit.jsp</result>
        </action>
        
        <action name="updateMasterplate" class="customerContractAction" method="updateMasterplate">
        	<interceptor-ref name="token" />
			<interceptor-ref name="defaultStack" />
			<result name="invalid.token">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateList.jsp</result>
            <result name="input">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateEdit.jsp</result>
        </action> 
        
        <action name="addProductMasterplateService" class="customerContractAction" method="addProductMasterplateService">
        	 <result name="input">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerProductAdd.jsp</result>
        </action>
        
        <action name="viewMasterplate" class="customerContractAction" method="viewMasterplate">
        	<result name="input">/WEB-INF/pages/univer/seller/customercontractmasterplate/customerContractMasterplateList.jsp</result>
        </action>
    </package>
</struts>
